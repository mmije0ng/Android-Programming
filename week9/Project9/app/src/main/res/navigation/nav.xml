<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.project9.Home"
        android:label="Home" >
        <action
            android:id="@+id/action_home_to_myDialogFragment"
            app:destination="@id/myDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/page1Fragment"
        android:name="com.example.project9.Page1"
        android:label="Frag1" />
    <fragment
        android:id="@+id/page2Fragment"
        android:name="com.example.project9.Page2"
        android:label="Frag2" />
    <dialog
        android:id="@+id/myDialogFragment"
        android:name="com.example.project9.Dialog"
        android:label="MyDialogFragment" />
</navigation>